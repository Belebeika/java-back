class Cat{
    String name;
    String mastername;
    int age;
    void Miu(){
        if (age < 1){
            System.out.printf("Мяяяяяяяу");
        }
        else{
            System.out.printf("Мьььяяууу");
        }
    }
    void Milk(){
        System.out.printf(name + " пьёт молоко");
    }

    void Maidan(){
        System.out.printf(name + " скачет");
    }

    void Getname(){
        System.out.printf(name);
    }

    void Getage(){
        System.out.println(age);
    }
    void displayInfo(){
        System.out.printf("Name: %s \tMastername: %s \tAge: %d\n", name, mastername, age);
    }
}

public class Main {
    public static void main(String[] args) {
        Cat tom = new Cat();
        tom.name = "Bazuka";
        tom.age = 10;
        tom.mastername = "Pavel";
        tom.Maidan();
    }
}
